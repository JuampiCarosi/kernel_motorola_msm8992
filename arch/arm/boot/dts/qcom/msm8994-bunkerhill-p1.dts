/dts-v1/;

#include "msm8994-v1-bunkerhill.dtsi"

/ {
	qcom,board-id = <0x41 0x8100>;
};

&i2c_5 {
	motosh@39 {
		gpios = <&msm_gpio 127 0>,      /* IRQ          */
			<&msm_gpio 57 0>,       /* RESET        */
			<&msm_gpio 78 0>,       /* BSLEN        */
			<&msm_gpio 65 0>,       /* WakeIRQ      */
			<&msm_gpio 89 0>,       /* SH Wake      */
			<&msm_gpio 16 0>;       /* SH Wake Resp */
	};
};

&tlmm_pinmux {
	pmx_motosh_out {
		qcom,pins = <&gp 57 &gp 78 &gp 89>;
		qcom,num-grp-pins = <3>;
	};
};

&i2c_2 {
	atmel_mxt_ts@4a {
		touch_vdd-supply = <&pm8994_l14>;
	};
};

&cci {
	qcom,camera@0 {
		gpios = <&msm_gpio 13 0>,
			<&msm_gpio 30 0>,
			<&msm_gpio 95 0>,
			<&msm_gpio 131 0>;
	};
};
